{
    "name": "dynamic/silverstripe-carousel",
    "description": "A simple carousel for Silverstripe websites.",
    "license": "BSD-3-Clause",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "require": {
        "silverstripe/linkfield":"^5.0",
        "silverstripe/recipe-cms": "6.x-dev",
        "symbiote/silverstripe-gridfieldextensions": "^5",
        "jonom/focuspoint": "dev-silverstripe6",
        "unclecheese/display-logic": "dev-silverstripe6"
    },
    "require-dev": {
        "dnadesign/silverstripe-elemental": "6.x-dev"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:muskie9/silverstripe-focuspoint.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:dynamic/silverstripe-display-logic.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dynamic\\Carousel\\": "src/",
            "Dynamic\\Carousel\\Test\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "3.x-dev"
        }
    }
}
